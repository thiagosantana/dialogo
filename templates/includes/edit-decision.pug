div(class="modal", id="vinter-modal-edit-decision")
    div(class="modal-content")
        span(class="close", id="close-edit-decision") &times;
        div(class="editor-field", id="vinter-div-decision-rule")
        button(type="button", id="vinter-btn-add-decision-rule", class="btn-form-field") ADD RULE
        div(class="vinter-btn-confirm")
            button(type="button", id="vinter-btn-edit-decision", class="btn-create") EDIT
            button(type="button", id="vinter-btn-confirm-delete-decision", class="btn-delete") DELETE